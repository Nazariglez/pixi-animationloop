{
  "name": "pixi-animationloop",
  "version": "1.0.5",
  "description": "AnimationLoop is a plugin for Pixi.js v3.0.8 or higher to manage the animation frame, doing the basic time operations, like calculate delta, total time, start/stop loop, and more.",
  "main": "./build/pixi-animationloop.js",
  "author": "Nazariglez <nazari.nz@gmail.com> (http://www.nazariglez.com)",
  "license": "MIT",

  "keywords": [
    "pixi",
    "animation",
    "delta",
    "requestanimationframe"
  ],

  "homepage": "https://github.com/Nazariglez/pixi-animationloop",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nazariglez/pixi-animationloop.git"
  },

  "scripts": {
    "prepublish" : "npm run build",
    "dev" : "webpack --config webpack.config.js --progress --inline --colors --watch",
    "build" : "NODE_ENV=production webpack --config webpack.config.js --progress --colors -p"
  },

  "devDependencies": {
    "pixi.js": "^3.0.8",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "brfs": "^1.4.1",
    "json-loader": "^0.5.4",
    "transform-loader": "^0.2.3",
    "webpack": "^1.12.9"
  }
}
